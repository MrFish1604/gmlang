{
    "name": "gmlang",
    "displayName": "GMlang",
    "description": "Support for GMlang",
    "icon": "./images/icon.png",
    "version": "0.0.2",
    "engines": {
        "vscode": ">=0.9.0-pre.1"
    },
    "publisher": "MrFish1604",

    "categories": ["Programming Languages"],

    "contributes": {
        "languages": [{
